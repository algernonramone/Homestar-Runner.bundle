<?xml version="1.0" encoding="UTF-8"?>
<site site="http://www.homestarrunner.com/*"
      plugin=".*?[^v]\.swf$"
      initialState="playing"
      version="1.0">
      
	<state name="playing">
		<event>
			<condition>
				<frameLoaded/>
			</condition>
			<action>
				<pause time="1000"/>
			</action>
		</event>
	</state>

</site>